{
  "root": "packages/minecraft-protocol-wrapper",
  "sourceRoot": "packages/minecraft-protocol-wrapper/src",
  "projectType": "library",
  "targets": {
    "generate": {
      "executor": "@nrwl/workspace:run-script",
      "outputs": ["src/generated"],
      "options": {
        "script": "generate"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "target": "generate",
          "projects": "self"
        }
      ],
      "options": {
        "outputPath": "dist/packages/minecrat-protocol-wrapper",
        "main": "packages/minecrat-protocol-wrapper/src/index.ts",
        "tsConfig": "packages/minecrat-protocol-wrapper/tsconfig.lib.json",
        "assets": ["packages/minecrat-protocol-wrapper/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/minecrat-protocol-wrapper/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/minecrat-protocol-wrapper"],
      "options": {
        "jestConfig": "packages/minecrat-protocol-wrapper/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
